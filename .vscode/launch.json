{
    "configurations": [
        {
            "name": "Debug With MSYS2 gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/ballPhysics.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ],
    "environment": [
        { "name": "path", "value": "${env:path};C:\\msys64\\ucrt64\\bin" }
    ],
    "version": "2.0.0"
}